{
  "containerDefinitions": [
    {
      "name": "kong-dashboard",
      "image": "pgbi/kong-dashboard:latest",
      "cpu": 256,
      "memory": 150,
      "command": ["start", "--kong-url", "http://ec2-35-172-225-250.compute-1.amazonaws.com:8001"],
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8081,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "family": "kong-dashboard"
}
