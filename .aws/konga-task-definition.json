{
  "containerDefinitions": [
    {
      "name": "konga",
      "image": "pantsel/konga:latest",
      "cpu": 512,
      "memory": 768,
      "portMappings": [
        {
          "containerPort": 1337,
          "hostPort": 1337,
          "protocol": "tcp"
        }
      ],
      "links": [
        "kongadmin:kongadmin"
      ],
      "environment": [
        {
          "name": "DB_ADAPTER",
          "value": "postgres"
        },
        {
          "name": "DB_HOST",
          "value": "kongdev.c5gdlu88ajos.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "DB_DATABASE",
          "value": "konga"
        },
        {
          "name": "DB_USER",
          "value": "kong"
        },
        {
          "name": "DB_PASSWORD",
          "value": "gJB78xXLjhS5BHL8vZOHwEyIc5M7M2jX"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    },
    {
      "name": "kongadmin",
      "image": "kong:latest",
      "cpu": 512,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 8001,
          "hostPort": 8001,
          "protocol": "tcp"
        },
        {
          "containerPort": 8444,
          "hostPort": 8444,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "KONG_DATABASE",
          "value": "postgres"
        },
        {
          "name": "KONG_PG_HOST",
          "value": "kongdev.c5gdlu88ajos.us-east-1.rds.amazonaws.com"
        },
        {
          "name": "KONG_PG_DATABASE",
          "value": "kong"
        },
        {
          "name": "KONG_PG_USER",
          "value": "kong"
        },
        {
          "name": "KONG_PG_PASSWORD",
          "value": "gJB78xXLjhS5BHL8vZOHwEyIc5M7M2jX"
        },
        {
          "name": "KONG_ADMIN_ACCESS_LOG",
          "value": "/dev/stdout"
        },
        {
          "name": "KONG_ADMIN_ERROR_LOG",
          "value": "/dev/stderr"
        },
        {
          "name": "KONG_ADMIN_LISTEN",
          "value": "0.0.0.0:8001"
        },
        {
          "name": "KONG_ADMIN_LISTEN_SSL",
          "value": "0.0.0.0:8444"
        }
      ]
    }
  ],
  "family": "konga"
}
